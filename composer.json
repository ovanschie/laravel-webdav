{
    "name": "pbmedia/laravel-webdav",
    "type": "library",
    "description": "Laravel 5 WebDAV Filesystem",
    "keywords": [
        "pbmedia",
        "laravel-webdav"
    ],
    "homepage": "https://github.com/pascalbaljetmedia/laravel-webdav",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "info@pascalbaljetmedia.com",
            "homepage": "https://www.pascalbaljetmedia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|^7.0",
        "illuminate/filesystem": "5.*",
        "league/flysystem-webdav": "1.0.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "phpunit/phpunit": "~5.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Pbmedia\\FilesystemProviders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pbmedia\\FilesystemProviders\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pbmedia\\FilesystemProviders\\WebDAVServiceProvider"
            ]
        }
    }
}
